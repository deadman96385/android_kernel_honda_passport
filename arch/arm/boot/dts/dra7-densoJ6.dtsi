                        0xCA4 (A_DELAY(155)   | G_DELAY(309))   /* CFG_VOUT1_D7_OUT */
                        0xCB0 (A_DELAY(212)   | G_DELAY(0))     /* CFG_VOUT1_D8_OUT */
                        0xCBC (A_DELAY(466)   | G_DELAY(0))     /* CFG_VOUT1_D9_OUT */
