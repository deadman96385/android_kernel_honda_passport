                        0xC20 (A_DELAY(516)   | G_DELAY(0))     /* CFG_VOUT1_D19_OUT */
                        0xC68 (A_DELAY(282)   | G_DELAY(0))     /* CFG_VOUT1_D2_OUT */
                        0xC38 (A_DELAY(386)   | G_DELAY(0))     /* CFG_VOUT1_D20_OUT */
                        0xC44 (A_DELAY(111)   | G_DELAY(0))     /* CFG_VOUT1_D21_OUT */
